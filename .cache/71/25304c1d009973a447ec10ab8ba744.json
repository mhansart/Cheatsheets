>\r\n    <pre>\r\n      <code>\r\n        <header></header>\r\n        <main></main>\r\n        <footer></footer>\r\n        <h1></h1>... <h6></h6>\r\n        <article></article>\r\n        <section></section>\r\n        <ul></ul>\r\n        <ol></ol>\r\n        <table></table>\r\n        <p></p>\r\n        <nav></nav>\r\n        <div></div>\r\n        <audio src=\"\"></audio>\r\n        <video src=\"\"></video>\r\n      </code>\r\n    </pre>\r\n    <h2>Les principales balises en ligne</h2>\r\n    <pre>\r\n      <code>\r\n        <img src=\"\" alt=\"\">\r\n        <button></button>\r\n        <span></span>\r\n        <em></em>\r\n        <strong></strong>\r\n        <br>\r\n      </code>\r\n    </pre>\r\n    <h2>Ressources</h2>\r\n    <article></article>\r\n    <script src=\"/9a9569e9d73f33740eada95275da7f30.js\"></script>\r\n  </body>\r\n</html>\r\n"},"sourceMaps":null,"error":null,"hash":"62a56098c845b591a109ffe10fe6900f","cacheData":{}}